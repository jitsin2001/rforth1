	processor pic18f248
	radix dec
	org 0x2000
	goto init_runtime
	org 0x2008
	reset
	org 0x2018
	reset

;---------------------------------------------------------
; Section: constants
;---------------------------------------------------------

RCREG equ 0xfae

;---------------------------------------------------------
; Section: code
;---------------------------------------------------------

init_runtime
	movlb 1
	clrf a,1
	clrf (b+1),1
	clrf b,1

main
	movf RCREG,w,0
	return

;---------------------------------------------------------
; Section: memory
;---------------------------------------------------------

a equ 0x100

b equ 0x101

	END
